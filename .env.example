# ============================================
# web2json-agent 配置文件示例
# ============================================
# 复制此文件为 .env 并填入实际的配置值
# cp .env.example .env

# ============================================
# API 配置 (OpenAI 兼容中转)
# ============================================
OPENAI_API_KEY=your_api_key_here
OPENAI_API_BASE=https://api.openai.com/v1

# ============================================
# LangSmith 配置（可选，用于追踪和监控）
# ============================================
# 启用 LangSmith 追踪（true/false）
LANGCHAIN_TRACING_V2=true
# LangSmith API 端点
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
# LangSmith API 密钥（从 https://smith.langchain.com 获取）
LANGCHAIN_API_KEY=your_langsmith_api_key_here
# LangSmith 项目名称
LANGCHAIN_PROJECT=web2json-agent

# ============================================
# 模型配置
# ============================================

# Agent 规划和执行
AGENT_MODEL=claude-sonnet-4-5-20250929
AGENT_TEMPERATURE=0

# 代码生成
CODE_GEN_MODEL=claude-sonnet-4-5-20250929
CODE_GEN_TEMPERATURE=0.3
CODE_GEN_MAX_TOKENS=16384

# 视觉理解（图片转JSON）
VISION_MODEL=qwen-vl-max
VISION_TEMPERATURE=0
VISION_MAX_TOKENS=4096

# ============================================
# Agent 配置
# ============================================

# 最大迭代次数（用于代码优化）
MAX_ITERATIONS=5

# 验证成功阈值（0.0-1.0，默认 80%）
SUCCESS_THRESHOLD=0.8

# 最小样本数量（建议 2-3 个）
MIN_SAMPLE_SIZE=2

# ============================================
# 浏览器配置
# ============================================

# 无头模式（true/false）
HEADLESS=true

# 页面加载超时（毫秒）
TIMEOUT=30000

# 截图是否包含完整页面（true/false）
SCREENSHOT_FULL_PAGE=true
